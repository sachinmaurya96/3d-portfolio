{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*).(js|css|svg|png|jpg)",
        "dest": "/$1.$2",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }